{
    "name" : "acelaya/zf2-acmailer",
    "description" : "Mail sending module for Zend Framework and Zend Expressive",
    "type" : "library",
    "authors" : [
        {
            "name" : "Alejandro Celaya Alastru√©",
            "email" : "alejandro@alejandrocelaya.com",
            "homepage" : "https://www.alejandrocelaya.com",
            "role" : "Developer"
        }
    ],
    "keywords" : [
        "zf2",
        "zf3",
        "expressive",
        "module",
        "mail",
        "zend"
    ],
    "homepage" : "https://github.com/acelaya/ZF-AcMailer",
    "license" : "MIT",
    "require" : {
        "php": "^7.0",
        "zendframework/zend-eventmanager": "^3.0",
        "zendframework/zend-expressive-template": "^1.0",
        "zendframework/zend-mail": "^2.6",
        "zendframework/zend-servicemanager": "^3.0",
        "zendframework/zend-stdlib": "^3.0"
    },
    "require-dev": {
        "http-interop/http-middleware": "^0.5.0",
        "phpunit/phpunit": "^6.0",
        "slevomat/coding-standard": "^4.0",
        "squizlabs/php_codesniffer": "^3.1",
        "zendframework/zend-expressive-zendviewrenderer": "^1.4",
        "zendframework/zend-mvc": "^3.1"
    },
    "suggest": {
        "zendframework/zend-expressive-zendviewrenderer": "If you want to render email templates using zend/view",
        "zendframework/zend-expressive-twigrenderer": "If you want to render email templates using twig/twig",
        "zendframework/zend-expressive-platesrenderer": "If you want to render email templates using league/plates"
    },
    "autoload" : {
        "psr-4" : {
            "AcMailer\\" : "src/"
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "AcMailerTest\\" : "test/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "zf": {
            "module": "AcMailer",
            "config-provider": "AcMailer\\ConfigProvider"
        }
     }
}
